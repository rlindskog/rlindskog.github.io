{
  "name": "rlindskog.github.io",
  "version": "1.0.0",
  "description": "My gh-pages site.",
  "author": "Ryan Lindskog <r.lindskog17@gmail.com>",
  "private": true,
  "dependencies": {
    "nuxt": "latest"
  },
  "scripts": {
    "deploy": "cd ./dist && git add . && git commit -m 'deploy' && git push -u -f origin master && cd ../",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && cd ./dist && git init && git remote add origin https://github.com/rlindskog/rlindskog.github.io.git && cd ../",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "push-dir": "^0.4.1"
  }
}
